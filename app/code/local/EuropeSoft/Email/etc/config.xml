<config>
    <modules>
        <EuropeSoft_Downloadable>
            <version>0.0.1</version>
        </EuropeSoft_Downloadable>
    </modules>
    <frontend>
          <routers>              
              <downloadable>
                  <args>
                    <modules>
                         <EuropeSoft_Downloadable before="Mage_Downloadable">EuropeSoft_Downloadable</EuropeSoft_Downloadable>
                    </modules>
                </args>
               
            </downloadable>
          </routers>
         
           <secure_url>
            
            <downloadable_download>/downloadable/download/</downloadable_download>
        </secure_url>
    </frontend>
</config>
